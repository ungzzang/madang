<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.green.madang.manager.book.BookMapper">
    <insert id="insBook">
        INSERT INTO book
           SET bookname= #{bookName}
             , publisher = #{publisher}
             , price = #{price}
    </insert>



</mapper>